<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="process_form" model="ir.ui.view">
        <field name="name">process.form</field>
        <field name="model">process</field>
        <field name="arch" type="xml">
            <form string="Processes">

            </form>
        </field>
    </record>

    <!--action of Processes menu-->
    <record model="ir.actions.act_window" id="action_process">
        <field name="name">Process</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">process</field>
        <field name="view_mode">form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No process found!
            </p>
        </field>
    </record>

    <menuitem id="mes_process" name="Processs" sequence="3" parent="ismart_mes_root" action="action_process"/>
</odoo>