<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_factory" model="ir.sequence">
            <field name="name">Sequence</field>
            <field name="code">ismart.mes.factory.sequence</field>
            <field name="prefix">F</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for process -->
        <record id="seq_process" model="ir.sequence">
            <field name="name">Process Sequence</field>
            <field name="code">ismart.mes.process.sequence</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
